{
  "Vendor": true,
  "Deadline": "2m",
  "Aggregate": true,
  "Sort": [
    "linter",
    "severity",
    "path",
    "line"
  ],
  "EnableGC": true,
  "Linters": {
    "nakedret": {
      "Command": "nakedret",
      "Pattern": "^(?P<path>.*?\\.go):(?P<line>\\d+)\\s*(?P<message>.*)$"
    }
  },
  "WarnUnmatchedDirective": true,
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "lll",
    "misspell",
    "nakedret",
    "unconvert",
    "unparam",
    "unused",
    "vet",
    "dupl",
    "vetshadow",
    "goconst",
    "gotype",
    "gotypex"
  ],
  "Cyclo": 21,
  "LineLength": 200,
  "DuplThreshold": 80
}
